{
  "configurations": [
    {
      "name": "const-param-propagate",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/svase",
      "cwd": "${workspaceRoot}",
      "args": ["test", "test/const-param-propagate/const-param-propagate_out.sv", "test/const-param-propagate/const-param-propagate.sv"],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
}